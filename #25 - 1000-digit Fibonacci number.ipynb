   "metadata": {},
   "outputs": [],
   "source": [
    "i = 1\n",
    "j = 1\n",
    "index= 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "while math.log(j,10)<999:\n",
    "    temp= j\n",
    "    j= i+j\n",
    "    i= temp\n",
    "    index+= 1"
   ]
