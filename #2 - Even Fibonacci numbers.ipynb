   "metadata": {},
   "outputs": [],
   "source": [
    "i= 1\n",
    "j= 2\n",
    "total = 0\n",
    "while j<4000000:\n",
    "    if(j%2==0):\n",
    "        total+=j\n",
    "    temp= j\n",
    "    j= i+j\n",
    "    i= temp"
