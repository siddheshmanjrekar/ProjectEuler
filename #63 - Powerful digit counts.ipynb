   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(2,10,1):\n",
    "    j= 1\n",
    "    while(math.ceil(math.log((i**j), 10))==j):\n",
    "        count+= 1\n",
    "        j+=1"
   ]
