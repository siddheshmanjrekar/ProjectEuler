   "source": [
    "## Digit fifth powers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "fifth_power_sum= 0"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i in range(2,360000,1):\n",
    "    total= 0\n",
    "    n= i\n",
    "    while(n>0):\n",
    "        r= n%10\n",
    "        total+= (r**5)\n",
    "        n= n//10\n",
    "    if(total== i):\n",
    "        fifth_power_sum+= i"
   ]
  },
